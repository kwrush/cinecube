    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "moviedb": "^0.2.10",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.3",
    "redux": "^3.7.2"
  }
}
